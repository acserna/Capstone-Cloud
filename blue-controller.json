{
    "kind":"ReplicationController",
    "apiVersion":"v1",
    "metadata":{
       "name":"acsr",
       "labels":{
          "app":"blue"
       }
    },
    "spec":{
       "replicas":1,
       "selector":{
          "app":"blue"
       },
       "template":{
          "metadata":{
             "labels":{
                "app":"blue"
             }
         },
         "spec":{
            "containers":[
               {
                  "name":"blue",
                  "image":"andrecamilosr/capstone-app-green-1f1411c1-a0fa-4546-8e0b-5af8a41698f8",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort": 80
                     }
                  ]
               }
            ]
         }
      }
   }
}